{
  "name": "netflix-clone",
  "homepage": "https://spay2024.github.io/CineNext",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "rm -rf dist .vite || true",
    "build": "vite build && cp dist/index.html dist/404.html",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
